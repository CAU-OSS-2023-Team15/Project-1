{
  "opts": {
    "template": "node_modules/minami",
    "destination": "./docs"
  },
  "source": {
    "include": ["./src", "./src/modules/PubSub.js", "./README.md"],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "plugins": [
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/typedef-import"
  ],
  "tags": {
    "allowUnknownTags": ["optional"]
  },
  "sourceType": "module"
}
